{
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "schemes": [
    "http"
  ],
  "swagger": "2.0",
  "info": {
    "description": "Our database management service, visualize in https://petstore.swagger.io/",
    "title": "Arcana.",
    "version": "1.0.0"
  },
  "host": "localhost:8081",
  "basePath": "/",
  "paths": {
    "/status": {
      "get": {
        "description": "checks if the serivce is reachable",
        "operationId": "status",
        "responses": {
          "200": {
            "$ref": "#/responses/StatusOK"
          }
        }
      }
    },
    "/v1/user/{used_id}/event": {
      "post": {
        "tags": [
          "event"
        ],
        "summary": "Creates a new event.",
        "operationId": "EventParam",
        "responses": {
          "201": {
            "$ref": "#/responses/Created"
          },
          "400": {
            "description": "Request",
            "$ref": "#/responses/Bad"
          },
          "500": {
            "description": "Server Error",
            "$ref": "#/responses/Internal"
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "basic": {
      "type": "basic"
    }
  }
}